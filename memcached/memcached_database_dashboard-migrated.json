  },
  "spec": {
    "display": {
      "name": "Memcached Overview"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "hidden": false
          },
          "defaultValue": "coreos-prom-memcd-stats",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "job",
              "matchers": [
                "memcached_up"
              ]
            }
          },
          "name": "job"
        }
      }
    ],
    "panels": {
      "0_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Instance State"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "0_1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Up Time"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "0_2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Connections"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "0_3": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "QPS"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "0_4": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Memory Usage Ratio"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "0_5": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Items"
          },
          "plugin": {
            "kind": "Markdown",
            "spec": {
              "text": "**Migration from Grafana not supported !**"
            }
          }
        }
      },
      "1_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "QPS"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "15s",
                    "query": "sum(delta(memcached_commands_total{job=\"coreos-prom-memcd-stats\"}[15m]))/15",
                    "seriesNameFormat": "QPS"
                  }
                }
              }
            }
          ]
        }
      },
      "1_1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Connections"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "memcached_current_connections{job=\"$job\"}",
                    "seriesNameFormat": "Connections"
                  }
                }
              }
            }
          ]
        }
      },
      "1_2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Read/Write"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "delta(memcached_read_bytes_total{job=\"$job\"}[1m])",
                    "seriesNameFormat": "read"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "delta(memcached_written_bytes_total{job=\"$job\"}[1m])",
                    "seriesNameFormat": "write"
                  }
                }
              }
            }
          ]
        }
      },
      "2_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Hit Ratio"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "sum (delta(memcached_commands_total{job=\"$job\", status=\"hit\"}[1m]))  / sum (delta(memcached_commands_total{job=\"$job\"}[1m])) * 100",
                    "seriesNameFormat": "Hit Ratio"
                  }
                }
              }
            }
          ]
        }
      },
      "2_1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Hit Ratio Per Command"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "sum (delta(memcached_commands_total{job=\"$job\", status=\"hit\",command=\"get\"}[1m]))  / sum (delta(memcached_commands_total{job=\"$job\",command=\"get\"}[1m])) * 100",
                    "seriesNameFormat": "get"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "sum (delta(memcached_commands_total{job=\"$job\", status=\"hit\",command=\"delete\"}[1m]))  / sum (delta(memcached_commands_total{job=\"$job\",command=\"delete\"}[1m])) * 100",
                    "seriesNameFormat": "delete"
                  }
                }
              }
            }
          ]
        }
      },
      "2_2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Command Total"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "sum(delta(memcached_commands_total{job=\"$job\"}[1m])) by (command)",
                    "seriesNameFormat": "{{ command }}"
                  }
                }
              }
            }
          ]
        }
      },
      "3_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "reclaimed / evicted"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "delta(memcached_items_reclaimed_total{job=\"$job\"}[1m])",
                    "seriesNameFormat": "reclaimed"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "1m",
                    "query": "delta(memcached_items_evicted_total{job=\"$job\"}[1m])",
                    "seriesNameFormat": "evicted"
                  }
                }
              }
            }
          ]
        }
      }
    },
