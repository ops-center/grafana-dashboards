{
  "kind": "Dashboard",
  "metadata": {
    "name": "wACsNpzIz",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Falco / Node Events"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "node",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "node",
              "matchers": [
                "kube_node_info"
              ]
            }
          },
          "name": "node"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Node FalcoEvents total",
            "description": "Total number of FalcoEvents occurred in node scope"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (node) (falco_appscode_com_events{node=\"$node\"})",
                    "seriesNameFormat": "{{ target_node }}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Node FalcoEvents by Priority",
            "description": "Total number of FalcoEvents occurred in node scope by priority"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (priority) (falco_appscode_com_events{node=\"$node\"})",
                    "seriesNameFormat": " {{ priority }}"
                  }
                }
              }
            }
          ]
        }
      },
      "2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Node FalcoEvents by Rule",
            "description": "Total number of FalcoEvents occurred in node scope by rule"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (rule) (falco_appscode_com_events{node=\"$node\"})",
                    "seriesNameFormat": "{{ rule }}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 15,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 15,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            },
            {
              "x": 0,
              "y": 15,
              "width": 24,
              "height": 16,
              "content": {
                "$ref": "#/spec/panels/2"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}