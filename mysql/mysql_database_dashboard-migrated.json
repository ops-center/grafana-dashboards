              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "rate(mysql_global_status_slow_queries{namespace=~\"$namespace\",service=~\"$app+-stats\"}[1m])",
                    "seriesNameFormat": "Slow Queries on {{ pod }}"
                  }
                }
              }
            }
          ]
        }
      }
