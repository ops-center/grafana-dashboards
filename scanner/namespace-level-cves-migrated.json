{
  "kind": "Dashboard",
  "metadata": {
    "name": "Edocn0hVz",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Scanner / Namespace CVEs"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "ns",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "namespace",
              "matchers": [
                "kube_namespace_created"
              ]
            }
          },
          "name": "ns"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Namespace CVE Occurance Total",
            "description": "Total number of vulnerabilities occurred in namespace scope"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (job, severity) (scanner_appscode_com_namespace_cve_occurrence_total{namespace=\"$ns\"})",
                    "seriesNameFormat": "{{severity}}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Namespace CVE Count Total",
            "description": "Number of unique vulnerabilities in namespace scope"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (job, severity) (scanner_appscode_com_namespace_cve_count_total{namespace=\"$ns\"})",
                    "seriesNameFormat": "{{severity}}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 16,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 16,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}