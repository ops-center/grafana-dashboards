{
  "kind": "Dashboard",
  "metadata": {
    "name": "Egwi40hVz",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Scanner / App CVEs"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "group",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "group",
              "matchers": [
                "apiserver_request_total{}"
              ]
            }
          },
          "name": "group"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "kind",
            "hidden": false
          },
          "defaultValue": "MongoDB",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "kind",
              "matchers": [
                "apiserver_watch_events_total{group=~\"$group\"}"
              ]
            }
          },
          "name": "kind"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "ns",
            "hidden": false
          },
          "defaultValue": "demo",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "namespace",
              "matchers": [
                "kube_namespace_created{}"
              ]
            }
          },
          "name": "ns"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "name",
            "hidden": false
          },
          "defaultValue": "mg",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "name",
              "matchers": [
                "scanner_appscode_com_image_lineage{group=~\"$group\",kind=~\"$kind\",namespace=~\"$ns\"}"
              ]
            }
          },
          "name": "name"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "App CVE Occurance Total",
            "description": "Severity distribution of vulnerability occurrences under a workload "
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by(severity) (scanner_appscode_com_image_cve_occurrence_total * on(image) group_left scanner_appscode_com_image_lineage{group=\"$group\",kind=\"$kind\",namespace=\"$ns\",name=\"$name\"})",
                    "seriesNameFormat": "{{ severity }}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "App CVE Count Total",
            "description": "Severity distribution of unique vulnerabilities under a workload "
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by(severity) (scanner_appscode_com_image_cve_count_total * on(image) group_left scanner_appscode_com_image_lineage{group=\"$group\",kind=\"$kind\",namespace=\"$ns\",name=\"$name\"})",
                    "seriesNameFormat": "{{ severity }}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 18,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 18,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}