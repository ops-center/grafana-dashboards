{
  "kind": "Dashboard",
  "metadata": {
    "name": "JAqfn0h4k",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Scanner / Image CVEs"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "img",
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "image",
              "matchers": [
                "scanner_appscode_com_image_cve_occurrence_total{}"
              ]
            }
          },
          "name": "img"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Image CVE Occurance Total",
            "description": "Severity distribution of total number of vulnerabilities for an image"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (severity) (scanner_appscode_com_image_cve_occurrence_total{image=\"$img\"})",
                    "seriesNameFormat": "{{severity}}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Image CVE Count Total",
            "description": "Severity distribution of unique vulnerabilities for an image"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (severity) (scanner_appscode_com_image_cve_count_total{image=\"$img\"})",
                    "seriesNameFormat": "{{severity}}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 18,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 18,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}