{
  "kind": "Dashboard",
  "metadata": {
    "name": "stTXBS-4z",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Policy / Namespace Violations"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "ns",
            "hidden": false
          },
          "defaultValue": "cert-manager",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "namespace",
              "matchers": [
                "kube_namespace_created"
              ]
            }
          },
          "name": "ns"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "constraint",
            "hidden": false
          },
          "defaultValue": "K8sContainerRequests",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "StaticListVariable",
            "spec": {
              "values": [
                "grafana",
                "migration",
                "not",
                "supported"
              ]
            }
          },
          "name": "constraint"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Namespace Policy violation Occurrence total",
            "description": "Total number of Gatekeeper policy violations occurred in namespace scope"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "policy_appscode_com_namespace_violation_occurrence_total{namespace=\"$ns\"}",
                    "seriesNameFormat": "{{ namespace }}"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Namespace Policy Violation occurrence  by Constraint type",
            "description": "Total number of policy violations occurred in namespace scope by constraint type"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "policy_appscode_com_namespace_violation_occurrence_by_constraint_type{namespace=\"$ns\",constraint=\"$constraint\"}",
                    "seriesNameFormat": "ns={{ namespace }}  , constraint= {{ constraint }}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 17,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 17,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}