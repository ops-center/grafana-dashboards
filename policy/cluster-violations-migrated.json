{
  "kind": "Dashboard",
  "metadata": {
    "name": "TWCN3-f4k",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "ACE / Policy / Cluster Violations"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "constraint",
            "hidden": false
          },
          "defaultValue": "K8sContainerRequests",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "StaticListVariable",
            "spec": {
              "values": [
                "grafana",
                "migration",
                "not",
                "supported"
              ]
            }
          },
          "name": "constraint"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Cluster Policy violation occurrence total",
            "description": "Total number of Gatekeeper policy violations occurred in cluster scope"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (job) (policy_appscode_com_cluster_violation_occurrence_total)",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Cluster Policy violation occurrence  by Constraint type",
            "description": "Total number of policy violations occurred in cluster scope by constraint type"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "sum by (constraint) (policy_appscode_com_cluster_violation_occurrence_by_constraint_type{constraint=\"$constraint\"})",
                    "seriesNameFormat": "{{ constraint }}"
                  }
                }
              }
            }
          ]
        }
      }
    },
    "layouts": [
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Panel Group 1"
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 14,
              "content": {
                "$ref": "#/spec/panels/0"
              }
            },
            {
              "x": 12,
              "y": 0,
              "width": 12,
              "height": 14,
              "content": {
                "$ref": "#/spec/panels/1"
              }
            }
          ]
        }
      }
    ],
    "duration": "1h"
  }
}